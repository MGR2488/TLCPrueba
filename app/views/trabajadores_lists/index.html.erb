<p id="notice"><%= notice %></p>

<h1>Trabajadores Lists</h1>

<table>
  <thead>
    <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trabajadores_lists.each do |trabajadores_list| %>
      <tr>
        <td><%= trabajadores_list.Nombres %></td>
        <td><%= trabajadores_list.Apellidos %></td>
        <td><%= link_to 'Show', trabajadores_list %></td>
        <td><%= link_to 'Edit', edit_trabajadores_list_path(trabajadores_list) %></td>
        <td><%= link_to 'Destroy', trabajadores_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trabajadores List', new_trabajadores_list_path %>
